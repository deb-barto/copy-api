{
    "version":2,
    "builds":[
        {
            "src":"backend/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python"
        }
    },
    {
        "src":"build.sh",
        "use":"@vercel/static-build",
        "config":{
            "disrDir":"staticfiles_build"
        }
    }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        }
        ,
        {
            "src":"/(.*)",
            "dest":"/backend/wsgi.py"
        }
    ]
}